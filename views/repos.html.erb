      <p><a href="<%= @repos[0]["owner"]["starred_url:"] %>">Stars</a></p>
      <p><a href="<%= @repos[0]["owner"]["followers_url:"] %>">Followers</a></p>
      <p><a href="<%= @repos[0]["owner"]["following_url:"] %>">Following</a></p>
    </nav>
    <ul>
      <p><a href="<%= @repos[0]["html_url:"] %>"><%= @repos[0]["name"] %></a></p>
